section.press-center
    .header-img
        a.top-banner_phone(href="tel:+74995530398") +7 499 553-03-98
        picture.lazyload
            source(media="(max-width: 768px)" data-srcset="img/service/header-mob.png")
            source(media="(min-width: 769px)" data-srcset="img/service/header-img-new.png")
            img(data-src="img/service/header-img-new.png" alt="header-img").lazyload
